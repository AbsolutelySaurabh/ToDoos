<resources>
    <!-- Base application theme. -->
    <style name="AppTheme.Base" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <!-- Blue -->
    </style>
    <!--<style name="AppTheme" parent="AppTheme.Base"></style>-->

    <style name="WithActionBar" parent="Theme.AppCompat.Light.DarkActionBar">

        <item name="android:windowActionBar">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="android:listDivider">@color/white</item>
    </style>


    <!-- Style for custom white radio button color -->
    <style name="WhiteRadioButton" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorControlNormal">@android:color/white</item>
        <item name="colorControlActivated">@android:color/white</item>
    </style>

    <style name="NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="colorPrimaryDark">@color/colorBlack</item>
        <item name="android:listDivider">@color/colorWhite</item>
    </style>

</resources>




